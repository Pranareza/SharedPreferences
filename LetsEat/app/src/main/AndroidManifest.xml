<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ayomakan.letseat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Let&apos;s Eat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Wisata.Jawa_Barat.Bekasi.amouzgourmet"
            android:label="@string/title_activity_amouzgourmet"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Wisata.Jawa_Barat.Bandung.restodior"
            android:label="@string/title_activity_restodior"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Wisata.Jawa_Barat.Bandung.atmosphere"
            android:label="@string/title_activity_atmosphere"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Wisata.Jawa_Barat.Bandung.braga"
            android:label="@string/title_activity_farm_house"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ListPlace.Wisata.Jawa_Barat.ListPlaceBandung" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>